{
  "name": "rescript-project-template",
  "version": "0.0.1",
  "scripts": {
    "build": "vite build",
    "clean": "rescript clean -with-deps",
    "format": "npm run format:js && npm run format:res",
    "format:js": "prettier --write \"**/*.{js,json,md,yml}\"",
    "format:res": "rescript format -all",
    "serve": "vite preview",
    "start": "vite"
  },
  "keywords": [
    "rescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@rescript/react": "^0.10.3",
    "esbuild": "^0.12.28",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rescript": "*"
  },
  "devDependencies": {
    "vite": "^2.5.10",
    "prettier": "^2.4.1"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "src/**/*.bs.js"
    ]
  }
}
